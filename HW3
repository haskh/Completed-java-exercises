//---------------------------------------------------------
void draw()
{ 
  background( 255 ,255,255) ; // white 
  fill( 0 , 0 , 0 ) ; 
  String errors = Assessment3.getLastErrorMessage(); 
  if( errors != null ) 
  { 
    text(errors, 20, 50);
  }  
 int cm = Assessment3.getCurrnetMarks() ; 
 
  text( " Total Marks so far = " + cm +"%", 20  , height - 30 ); 
} 
