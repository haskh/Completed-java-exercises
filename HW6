  class HW6cClass {
  String FirstName; 
  String LastName;
  String StudentID;
  boolean Marked;
  int AttendedNWeeks;
  HW6cClass() {
    this.FirstName="Jenny";
    this.LastName="Banks";
    this.StudentID="WF230230293";
    this.Marked=true;
    this.AttendedNWeeks=5;
  }
}
public HW6cClass HW6C( )
{ 
  HW6cClass Jenny = new HW6cClass(); 
  return Jenny ;
}
