// YOUR CLASS HERE.
   class HW6eClass{
  String ProductName;
  int number;
  float price;
  public float myValuePlusVAT(){
    float subTotal=number*price;
    float VAT=subTotal*0.2;
    return subTotal+VAT;
  }  
}

public HW6eClass HW6E( )
{ 
  HW6eClass it = new HW6eClass() ;
  it.ProductName="Cheese";
  it.number=1;
  it.price=165;
 
  return it ;
}
