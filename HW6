     /* Note for this template change Object type to your class HW6dClass name */ 
   public class HW6dClass {
  String FirstName;
  String LastName;
  String StudentID;
  float Height;
  int HatSize;
}
public ArrayList<HW6dClass>  HW6D(  )
{ 
  ArrayList<HW6dClass> list = new ArrayList<HW6dClass>( ) ;
  HW6dClass Banks= new HW6dClass();
  Banks.FirstName="Jenny";
  Banks.LastName="Banks";
  Banks.StudentID="230230293";
  Banks.Height=1.8;
  Banks.HatSize=8;
  HW6dClass Rockwood= new HW6dClass();
  Rockwood.FirstName="Brooklyn";
  Rockwood.LastName="Rockwood";
  Rockwood.StudentID="90943049";
  Rockwood.Height=2.1;
  Rockwood.HatSize=12;
  HW6dClass YourName=new  HW6dClass();
  YourName.FirstName="YourName";
  YourName.LastName="YourName";
  YourName.StudentID="330230193";
  YourName.Height=2.0;
  YourName.HatSize=12;
  list.add(Banks);
  list.add(Rockwood);
  list.add(YourName);
  return list;
}
